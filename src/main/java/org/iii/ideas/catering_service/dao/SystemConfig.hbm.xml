<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="org.iii.ideas.catering_service.dao.SystemConfig" table="systemconfig" >
    	
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="pname" type="java.lang.String">
            <column name="pname" not-null="true" />
        </property>
        <property name="pvalue" type="java.lang.String">
            <column name="pvalue" not-null="true" />
        </property>
        <property name="ptype" type="java.lang.String">
            <column name="ptype" not-null="true" />
        </property>
        <property name="penable" type="java.lang.String">
            <column name="penable" not-null="true" />
        </property>       
    </class>
</hibernate-mapping>
